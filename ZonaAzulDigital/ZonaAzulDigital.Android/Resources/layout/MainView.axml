<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:textAlignment="center">
    <ImageView
        android:src="@drawable/cet3"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:id="@+id/imageView1"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp" />
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:numeric="integer"
        android:hint="CPF"
        android:textColorHint="#808080"
        local:MvxBind="txtCPF Text" />
    <Space
        android:layout_width="match_parent"
        android:layout_height="30.0dp"
        android:id="@+id/space1" />
    <EditText
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:password="true"
        android:hint="Senha"
        android:textColorHint="#808080"
        local:MvxBind="txtSenha Text"
        android:id="@+id/editText1" />
    <Space
        android:layout_width="match_parent"
        android:layout_height="30.0dp"
        android:id="@+id/space1" />
    <Button
        android:layout_width="179.0dp"
        android:layout_height="54.0dp"
        android:text="ENTRAR"
        android:textSize="30dp"
        local:MvxBind="Click LoginTextCommand"
        android:layout_gravity="center"
        android:textColor="#ffffff"
        android:background="#099bdd" />
    <Space
        android:layout_width="match_parent"
        android:layout_height="30.0dp"
        android:id="@+id/space1" />
    <Button
        android:layout_width="179.0dp"
        android:layout_height="54.0dp"
        android:text="CADASTRO"
        android:textSize="30dp"
        local:MvxBind="Click CadastroTextCommand"
        android:layout_gravity="center"
        android:background="#099bdd"
        android:textColor="#ffffff"
        android:id="@+id/button1" />
</LinearLayout>